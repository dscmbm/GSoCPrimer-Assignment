import numpy as np
mat = np.arange(1, 10).reshape(3, 3)
print(mat)



ANSWER 6
def fact(n):
    if n==1:
        return 1
    else:
        return n*fact(n-1)
x=int(input("Enter a positive number: "))
print("Factorial of",x,"is",fact(x))



