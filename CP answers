question 2:

#include <iostream>

using namespace std;

int main()
{
   int a;
   cout<<"Enter a number: "<<endl;
   cin>>a;
   if(a%2==0)
   cout<<"Number is even "<<endl;
   else
   cout<<"Number is odd "<<endl;

    return 0;
}
question 3:
#include <iostream>

using namespace std;

int main()
{
   int a;
   cout<<"Enter a number: "<<endl;
   cin>>a;
   int fact=1;
   for(int i=1;i<=a;i++)
   {
       fact=fact*i;
   }
   cout<<fact<<endl;
   
    return 0;
}
question 4:
#include <iostream>
#include <stdio.h>

using namespace std;

int main()
{
    //char str[100] = "Vandanasaini";
    string str;
    cin>>str;
    int vowels = 0;
    
    
    for(int i = 0; str[i]; i++)  
    {
        if(str[i]=='a'|| str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'
        ||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O' ||str[i]=='U')
        {
		    vowels++;
        }
    }
    
    cout << "Total Vowels : " << vowels;
    
    return 0;
}
question 5:


#include <iostream>

using namespace std;

int main()
{
   int n;
   cout<<"Enter a number in array: "<<endl;
   cin>>n;
   int max=0;
   int  arr[n];
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   for(int i=0;i<n;i++)
   {
       if(max<arr[i])
       {
           max=arr[i];
       }
       else
       {
           max=max;
       }
       
   }
   cout<<max;
    return 0;
}
Question 6: 


#include <iostream>

using namespace std;

int main()
{
   int n;
   cout<<"Enter a number in array: "<<endl;
   cin>>n;
   int min=99;
   int  arr[n];
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   for(int i=1;i<n;i++)
   {
       if(min>arr[i])
       {
           min=arr[i];
       }
       else
       {
           min=min;
       }
       
   }
   cout<<min;
    return 0;
}
