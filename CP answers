    cout<<str<<endl;
//Q10
    int l,k,j;
    cout<<"enter int"<<endl;
    cin>>l>>k>>j;
    int maxnumber=(l>k) ? (l>j ? l : j) : (k>j ? k:j);
    cout<<maxnumber<<endl;
}
