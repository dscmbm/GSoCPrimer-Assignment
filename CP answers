// WAP
// Q.1
#include<iostream>
using namespace std;
int main{
    int a,b;
    cin>>a>>b;
    cout<<a*b;
    return 0;
}
// Q.2
#include<iostream>
using namespace std;
int main{
    int a;
    cin>>a;
    if(a % 2 == 0){
        cout<<"a is an even number.";
    }
    else{
        cout<<"a is an odd number.";
    }
    return 0;
}
// Q.3
#include<iostream>
using namespace std;
int fact(int n){
    if(n == 0){
        return 1;
    }
    if(n == 1){
        return 1;
    }
    int factNm1 = fact(n-1);
    int factNm2 = fact(n-2);
    int factn = factNm1 + factNm2;
    return factn;
}
int main{
    int n;
    cin>>n;
    cout<<fact(n);
    return 0;
}
