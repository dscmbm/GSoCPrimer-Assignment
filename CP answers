#include<iostream>
using namespace std;
int main()
{
    int a,b,c,greter;

    cout<<"enter three number : "<<endl;
    cin>>a>>b>>c;

    if(a>=b && a>=c)
        cout<<"The greter number is :"<<a;
        
    else if(b>=a && b>=c)
        cout<<"The greter number is :"<<b;

    
    else
        cout<<"The greter number is :"<<c;

    return 0;  



}

