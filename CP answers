Ques 1.cp
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter two numbers a and b"<<endl;
    cin>>a>>b;
    c=a*b;
    cout<<"multiplication of a and b is"<<c<<endl;
}

Ques 2.cp
#include<iostream>
using namespace std;
int main(){
    int num;
    cout<<"Enter the number"<<endl;
    cin>>num;
    if(num%2==0){
        cout<<"Number is even"<<endl;
    }
    else{
        cout<<"Number is odd"<<endl;
    }
}
Ques 3.cp
#include<iostream>
using namespace std;
int main(){
    int n,fact=1;
    cout<<"Enter the number"<<endl;
    cin>>n;
    for(int i=1;i<n;i++){
        fact=fact*i;
    }
    cout<<"Factorial of "<<n<<" is "<<fact<<endl;
    return 0;
}
Ques 7.cp
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter two number to be swapped"<<endl;
    cin>>a>>b;
    c=a;
    a=b;
    b=c;
    cout<<"After swapping the numbers "<<a<<" "<<b<<endl;
}
Ques 10.cp
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter three numbers"<<endl;
    cin>>a>>b>>c;
    //if a is greater than b and c
    if(a>b && a>c)
        cout<<"Max is "<<a<<endl;
    //if b is greater than c and a
    else if(b>c&&b>a)
        cout<<"Max is "<<b<<endl;
    //if c is greater than a and b
    else
        cout<<"Max is "<<c<<endl;
}

