ans 2-
#include<iostream>
using namespace std;


int main()
{
 int n;
 cout<<"Enter the number :";
 cin>>n;

 if(n%2==0){
    cout<<"Given input is even number";


 }
 else
    cout<<"Given input is not even number";

    return 0;

}
