//coding problems
//q1
#include <iostream>
using namespace std;
int main(){
    int a, b;
    cin>>a>>b;
    
    cout<<a*b;
    return 0;
}
//q2
int main(){
    int num;
    if (num%2==0){
        cout<<"even"<< endl;
    else{
        cout<<"odd";
    } 
    }
}

//q3
int main(){
    int num, fact=1;
    for(i=1; i<=num;i ++){
        fact=fact*i;
    }
    cout<<fact;
    if (num==0){
        return 1;
    }
}
//q5
int main(){
    int arr[5]={1,2,3,4,5};
    int i,max=0;
    for (i=0; i<5;i++){
        if(arr[i]<arr[i+1]){
            max=arr[i+1];
        }
        cout<< max;
    }   
}

//q7
int main(){
    int a,b, temp;
    cin>>a>>b;
    a=b;
    b=temp;
    temp=a;
}

