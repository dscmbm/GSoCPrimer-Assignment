<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cheack-box</title>
</head>
<body>
    <div class="chackBox-field" >
        <div>
            <input type="checkbox" id="check1"/>
            <label for="check1">First option</label>
        </div>

        <div>
            <input type="checkbox" id="check1"/>
            <label for="check1">Second option</label>
        </div>

        <div>
            <input type="checkbox" id="check1"/>
            <label for="check1">Third option</label>
        </div>
        
    </div>
</body>
</html>


